script/source = "extends StaticBody

# механизм действий
var actions: CActions;

# узел готов
func _ready() -> void:
	# поднимаем механизм действий
	actions = CActions.new(self);
	
	# регаем действие \"уничтожить\"
	actions.set_action(CActionsWallDestroy.new(\"destroy\"));

# раз в кадр
func _physics_process(delta: float):
	
	# если есть центральное действие
	if true == actions.has_current_action():
		# исполняем его
		actions.get_current_action().process(delta);
		
# нанести урон
func make_damage(delta: float) -> void:
	# стартуем действие \"уничтожить стену\"
	actions.set_current_action(\"destroy\", delta);
"

